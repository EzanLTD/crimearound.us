<form>
    <fieldset>
        <legend>Filter by date</legend>
        <input class="filter start" type="text" value="Start Date: <%= moment().subtract('d', 14).format('YYYY-MM-DD') %>" placeholder="Start Date"/>
        <input class="filter end" type="text" value="End Date: <%= moment().subtract('d', 7).format('YYYY-MM-DD') %>" placeholder="End Date"/>
    </fieldset>
    <select data-placeholder="Apply filters" class="chosen-select" multiple>
        <option value=""></option>
        <optgroup label="Location Description">
            <option value="inside">Inside</option>
            <option value="street">Street</option>
            <option value="vacant">Vacant Property</option>
        </optgroup>
    </select>
</form>
